<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>


<StackPane fx:id="rootPane" prefHeight="180.0" prefWidth="210.0" stylesheets="@../css/dark-theme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.home_books_javafx_spring.ui.SettingsController">
   <children>
      <AnchorPane fx:id="rootAnchorPane" prefHeight="180.0" prefWidth="210.0">
         <children>
            <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <JFXButton fx:id="statusTypeButton" onAction="#showStatusTypeList" prefHeight="40.0" prefWidth="170.0" text="Status Type List">
                     <VBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </VBox.margin>
                  </JFXButton>
                  <JFXButton fx:id="roomListButton" onAction="#showRoomList" prefHeight="40.0" prefWidth="170.0" text="Room List">
                     <VBox.margin>
                        <Insets left="20.0" right="20.0" top="30.0" />
                     </VBox.margin>
                  </JFXButton>
                  <JFXButton fx:id="shelfListButton" onAction="#showShelfList" prefHeight="40.0" prefWidth="170.0" text="Shelf List">
                     <VBox.margin>
                        <Insets left="20.0" right="20.0" top="30.0" />
                     </VBox.margin>
                  </JFXButton>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</StackPane>
